{
    "name": "latinosoft/laravel-tileserver",
    "description": "Turn your Laravel app into an XYZ tile server using your own mbtiles resources.",
    "keywords": [
        "teamzac",
        "latinosoft", 
        "laravel-tileserver"
    ],
    "homepage": "https://github.com/teamzac/laravel-tileserver",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Chad Janicek",
            "email": "chad@zactax.com",
            "role": "Developer"
        },
        {
            "name": "Juan Noriega",
            "email": "jsnoriegam@hotmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.3|^8.0",
        "illuminate/support": "^8.0|^9.0"
    },
    "require-dev": {
        "orchestra/testbench": "^6.0",
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "LaravelTileserver\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "LaravelTileserver\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"

    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "LaravelTileserver\\LaravelTileserverServiceProvider"
            ]
        }
    }
}
